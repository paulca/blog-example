<h1>Listing cell_phones</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Manufacturing date</th>
    <th>Description</th>
    <th>In stock</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cell_phones.each do |cell_phone| %>
  <tr data-id="<%= cell_phone.id %>">
    <td><%= cell_phone.title %></td>
    <td><%= cell_phone.manufacturing_date %></td>
    <td><%= cell_phone.description %></td>
    <td><%= cell_phone.in_stock %></td>
    <td><%= link_to 'Show', cell_phone, :class => 'show-phone' %></td>
    <td><%= link_to 'Edit', edit_cell_phone_path(cell_phone) %></td>
    <td><%= link_to 'Destroy', cell_phone, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cell phone', new_cell_phone_path %>


<div id="frame"></div>